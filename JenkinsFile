pipeline {
    stages {
        stage('Cloning Git') {
            steps {
                git branch: 'main', credentialsId: 'myid', url: 'https://github.com/AlexisBozel/webapi.git'
            }
        }
        stage('Build image') {
            steps {
                script {
                    container("docker"){
                        docker.build("totank4545/st2dce-prj:latest")
                    }
                }
            }
        }
    }
}